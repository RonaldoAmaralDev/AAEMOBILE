<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background= "@color/colorShop">

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-5dp"
        android:layout_marginTop="176dp"
        android:background="@color/white" />

    <ImageButton
        android:id="@+id/foto3a"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/textView14"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-207dp"
        android:layout_marginBottom="20dp"
        android:adjustViewBounds="true"
        android:background="@drawable/semfoto"
        android:scaleType="centerCrop" />

    <ImageButton
        android:id="@+id/foto2a"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/textView14"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-205dp"
        android:layout_marginBottom="122dp"
        android:adjustViewBounds="true"
        android:background="@drawable/semfoto"
        android:scaleType="centerCrop" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="11dp"
        android:layout_marginLeft="11dp"
        android:layout_marginTop="26dp"
        android:text="Atividade:"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/txtAtividade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView6"
        android:layout_marginStart="-4dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/textView14"
        android:text="Realizar manutenção corretiva"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_marginTop="63dp"
        android:text="Localização:"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/txtLatitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView14"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="107dp"
        android:layout_marginLeft="107dp"
        android:layout_marginTop="0dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/txtLongitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/txtLatitude"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="70dp"
        android:layout_marginRight="70dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="14sp" />

    <View
        android:id="@+id/divider7"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-6dp"
        android:layout_marginTop="92dp"
        android:background="@color/white" />

    <ImageButton
        android:id="@+id/foto1a"
        android:layout_width="180dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/textView14"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="-205dp"
        android:layout_marginBottom="226dp"
        android:adjustViewBounds="true"
        android:background="@drawable/semfoto"
        android:scaleType="centerCrop" />

    <EditText
        android:id="@+id/txtObservacaoAntes"
        android:layout_width="416dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/txtLongitude"
        android:layout_alignParentTop="true"
        android:layout_marginTop="111dp"
        android:layout_marginEnd="-75dp"
        android:ems="10"
        android:hint="Inserir Observação"
        android:inputType="textMultiLine"
        android:textColor="@color/white"
        android:textColorHint="@color/white"
        android:textSize="14sp" />

</RelativeLayout>